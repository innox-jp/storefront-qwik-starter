{
	"compilerOptions": {
		"allowJs": true,
		"target": "ES2022",
		"module": "ES2022",
		"lib": ["es2020", "DOM", "WebWorker", "DOM.Iterable"],
		"jsx": "react-jsx",
		"jsxImportSource": "@builder.io/qwik",
		"strict": true,
		"declaration": true,
		"declarationDir": "lib/types",
		"resolveJsonModule": true,
		"moduleResolution": "node",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"incremental": true,
		"isolatedModules": true,
		"types": ["vite/client"],
		"paths": {
			"~/*": ["./src/*"]
		}
	},
	"include": ["src"]
}
