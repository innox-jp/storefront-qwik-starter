{
	"name": "storefront-qwik-starter",
	"description": "File based routing and MDX support Vite.js tooling. Cloudflare Pages. Prettier code formatter. Tailwind CSS framework.",
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build && npm run i18n-translate",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.server": "vite build -c adapters/deno/vite.config.ts",
		"build.types": "tsc --incremental --noEmit",
		"build.deno": "npm run build.client && vite build -c adapters/deno/vite.config.ts",
		"deploy": "wrangler pages dev ./dist",
		"dev": "vite --mode ssr --port 8080",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"dev.server": "cd one-click-deploy && yarn dev",
		"develop": "concurrently pnpm:dev pnpm:dev.server",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"generate": "graphql-codegen --config codegen.yml",
		"i18n-extract": "node_modules/.bin/localize-extract -s \"dist/build/*.js\" -f json -o src/locales/message.en.json",
		"i18n-sort": "ts-node --project scripts/tsconfig.scripts.json scripts/sort-i18n.ts src/locales",
		"i18n-translate": "bash node_modules/.bin/localize-translate -s \"*.js\" -t src/locales/message.*.json -o dist/build/{{LOCALE}} -r ./dist/build",
		"lint": "eslint \"src/**/*.ts*\"",
		"prepare": "husky install",
		"preview": "qwik build preview && vite preview --open",
		"serve": "deno run --allow-net --allow-read --allow-env server/entry.deno.js",
		"start": "vite --open --mode ssr",
		"qwik": "qwik"
	},
	"devDependencies": {
		"@angular/compiler": "^16.0.4",
		"@angular/compiler-cli": "^16.0.4",
		"@builder.io/qwik": "1.1.5",
		"@builder.io/qwik-city": "1.1.5",
		"@graphql-codegen/cli": "^4.0.1",
		"@graphql-codegen/introspection": "^4.0.0",
		"@graphql-codegen/typescript": "^4.0.0",
		"@graphql-codegen/typescript-operations": "^4.0.0",
		"@tailwindcss/forms": "^0.5.3",
		"@types/braintree-web-drop-in": "^1.34.2",
		"@types/eslint": "8.40.0",
		"@types/node": "latest",
		"@typescript-eslint/eslint-plugin": "5.59.9",
		"@typescript-eslint/parser": "5.59.9",
		"autoprefixer": "10.4.14",
		"concurrently": "8.1.0",
		"eslint": "8.42.0",
		"eslint-plugin-qwik": "1.1.5",
		"husky": "^8.0.3",
		"node-fetch": "3.3.1",
		"postcss": "8.4.24",
		"prettier": "2.8.8",
		"pretty-quick": "^3.1.3",
		"tailwindcss": "3.3.2",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vite-tsconfig-paths": "4.2.0",
		"wrangler": "latest"
	},
	"engines": {
		"node": ">=16"
	},
	"homepage": "https://qwik.builder.io/",
	"license": "",
	"private": true,
	"type": "module",
	"dependencies": {
		"@angular/localize": "^16.0.4",
		"@builder.io/qwik-labs": "github:BuilderIo/qwik-labs-build#qwik-labs",
		"@graphql-codegen/typescript-generic-sdk": "^3.1.0",
		"@stripe/stripe-js": "^1.54.0",
		"braintree-web-drop-in": "^1.38.0",
		"graphql": "^16.6.0",
		"graphql-tag": "^2.12.6",
		"qwik-image": "^0.0.6",
		"undici": "^5.22.1",
		"zod": "^3.21.4"
	},
	"overrides": {
		"@angular/compiler-cli": {
			"typescript": "5.1.3"
		}
	}
}
